<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chandreshgupta.timetable">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".FacultyActivity"></activity>
        <activity android:name=".StudentActivity"></activity>
        <activity android:name=".DisplaySemesterTimetable"></activity>
        <activity android:name=".DisplayFacultyTimetable"></activity>
        <activity android:name=".ActivityTabSemester" />
        <activity android:name=".ActivityTabFaculty" />
    </application>

</manifest>